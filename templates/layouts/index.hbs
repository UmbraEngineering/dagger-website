<!doctype html>
<html>
<head>
	
	<meta charset="utf-8" />
	<title>Dagger.js</title>

	<link rel="stylesheet" type="text/css" href="/fonts/district-thin/stylesheet.css" />
	<link rel="stylesheet" type="text/css" href="/build/css/styles.min.css" />

</head>
<body>

	<div id="wrapper">
		<div id="header">
			<h1><a href="/#">Dagger.js</a></h1>
			<nav>
				<ul>
					<li><a href="/#guides">Guides</a></li>
					<li><a href="/#docs">Documentation</a></li>
					<li><a href="https://github.com/UmbraEngineering/dagger">GitHub</a></li>
					<li><a href="https://github.com/UmbraEngineering/dagger/issues">Issues</a></li>
				</ul>
			</nav>
		</div>
		<div id="content">
			{{#if wrapper}}{{{ wrapper.[0] }}}{{/if}}
			{{{ body }}}
			{{#if wrapper}}{{{ wrapper.[1] }}}{{/if}}
		</div>
		<div id="footer">
			Copyright &copy; {{ year }} <a href="http://www.umbraengineering.com">Umbra Engineering LLC</a><br />
			Dagger.js code licensed under <a href="http://opensource.org/licenses/MIT">MIT</a>. This site and documentation
			licensed under <a href="http://creativecommons.org/licenses/by/3.0/">CC BY 3.0</a>.
		</div>
	</div>
	
	<script src="/build/js/common{{#if production}}.min{{/if}}.js"></script>
	<script src="/build/js/lib{{#if production}}.min{{/if}}.js"></script>
	<script src="/build/js/app{{#if production}}.min{{/if}}.js"></script>

	<script>

		// Set the require paths
		require.paths.push('/lib', '/vendor');

		// Use <section> elements for views
		require('cloak').config.viewTag = 'section';

		// Load JSON polyfill if needed
		JSON || require('json2');

		// Load our handlebars helpers
		require('helpers.handlebars');

		// Load the router
		var Router = require('routers/main');
		var router = new Router({ autoStart: false }).start();

	</script>

</body>
</html>